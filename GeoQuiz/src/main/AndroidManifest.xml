<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aliyastuff.geoquiz" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.aliyastuff.geoquiz.QuizActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.aliyastuff.geoquiz.QuizActivity$Cheat"
            android:label="@string/title_activity_cheat" >
        </activity>
        <activity
            android:name="com.aliyastuff.geoquiz.Cheat"
            android:label="@string/title_activity_cheat"
            android:parentActivityName="com.aliyastuff.geoquiz.QuizActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="QuizActivity" />
        </activity>
    </application>

</manifest>
